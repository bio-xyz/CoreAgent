{
    "compilerOptions": {
      "target": "ES2019",
      "module": "CommonJS",
      "moduleResolution": "node",
      "outDir": "dist",
      "rootDir": "src",
      "strict": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src/**/*"]
  }